    }\r\n    }, [user]);\r\n\r\n    // console.log(\"this is the basket >>>\", basket);\r\n\r\n    const add_to_basket = () => {\r\n\r\n        // Dispatch the item into the data layer\r\n        dispatch({\r\n\r\n            type: 'ADD_TO_BASKET',\r\n            item: {\r\n\r\n                id: id,\r\n                title: title,\r\n                image: image,\r\n                price: price, \r\n                rating: rating,\r\n            },\r\n        });\r\n    };\r\n\r\n    return (\r\n        <div className=\"product\">   \r\n            <div className=\"product-info\">\r\n                <p>{title}</p>\r\n\r\n                <p className=\"product-price\">\r\n                    <small>$</small>\r\n                    <strong>{price.toFixed(2)}</strong>\r\n                </p>\r\n\r\n                <div className=\"product-rating\">\r\n                    {Array(rating).fill().map((_, i) => {\r\n                        \r\n                        return <p>⭐</p>\r\n                    })}\r\n                </div>\r\n            </div>\r\n\r\n            <img src={image} alt=\"\"/>\r\n\r\n            <button onClick={add_to_basket}>Add to Basket</button>\r\n        </div>\r\n    )\r\n};\r\n\r\nexport default Product;\r\n","C:\\Users\\FS Web Dev\\Desktop\\Web_Dev_Main\\Other_Projects\\Amazon_React_Clone\\amazon-clone\\src\\Checkout.jsx",["89"],"import React, { useEffect } from 'react';\r\nimport \"./Checkout.css\";\r\nimport CheckoutProduct from './CheckoutProduct.jsx';\r\nimport { UseStateValue } from './StateProvider.jsx';\r\nimport Subtotal from './Subtotal.jsx';\r\n\r\nfunction Checkout() {\r\n\r\n    const [{basket, user}, dispatch] = UseStateValue();\r\n\r\n    useEffect(() => {\r\n        \r\n        if(!user)\r\n        {\r\n            dispatch({\r\n\r\n                type: 'EMPTY_BASKET',\r\n            })\r\n        }\r\n    }, [user]);\r\n\r\n    return (\r\n        <div className=\"checkout\">\r\n            <div className=\"checkout-left\">\r\n                <img src=\"https://images-na.ssl-images-amazon.com/images/G/02/UK_CCMP/TM/OCC_Amazon1._CB423492668_.jpg\" alt=\"\" className=\"checkout-ad\"/>\r\n            \r\n                <div>\r\n                    <h3>Hello, {user?.email ? user.email : \"Guest\"}</h3>\r\n                    <h2 className=\"checkout-title\">Your Shopping Basket</h2>\r\n\r\n                    {basket.map(item => (\r\n\r\n                        <CheckoutProduct \r\n                            id = {item.id}\r\n                            title = {item.title}\r\n                            image = {item.image}\r\n                            price = {item.price}\r\n                            rating = {item.rating}\r\n                        />\r\n                    ))}\r\n\r\n                    {/* <CheckoutProduct \r\n                        id = '112342' //{item.id}\r\n                        title = 'This is a test, bla bla bla bla bla bla bla bla bla bla bla bla bla' //{item.title}\r\n                        image = 'https://m.media-amazon.com/images/I/51WIKlio9qL.jpg' //{item.image}\r\n                        price = {199.99} //{item.price}\r\n                        rating = {5} //{item.rating}\r\n                    /> */}\r\n                </div>\r\n            </div>\r\n\r\n            <div className=\"checkout-right\">\r\n                <Subtotal />\r\n                {/*<h2>The subtotal will go here</h2>*/}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Checkout;\r\n","C:\\Users\\FS Web Dev\\Desktop\\Web_Dev_Main\\Other_Projects\\Amazon_React_Clone\\amazon-clone\\src\\Subtotal.jsx",["90"],"import React from 'react';\r\nimport \"./Subtotal.css\";\r\nimport CurrencyFormat from \"react-currency-format\";\r\nimport { UseStateValue } from './StateProvider.jsx';\r\nimport { get_basket_total } from './reducer.js';\r\nimport { useHistory } from 'react-router-dom';\r\n\r\nfunction Subtotal() {\r\n\r\n    const history = useHistory();\r\n    const [{basket}, dispatch] = UseStateValue();\r\n\r\n    return (\r\n        <div className=\"subtotal\">\r\n            <CurrencyFormat \r\n                renderText={(value) => (\r\n                    <>\r\n                        <p>\r\n                            {/* Part of HW */}\r\n                            Subtotal ({basket?.length} items):\r\n                            <strong> {value}</strong>\r\n                        </p>\r\n                        <small className=\"subtotal-gift\">\r\n                            <input type=\"checkbox\" /> This order contains a gift\r\n                        </small>\r\n                    </>\r\n                )}\r\n\r\n                decimalScale={2}\r\n                value={get_basket_total(basket)} //Part of HW\r\n                displayType={\"text\"}\r\n                thousandSeparator={true}\r\n                prefix={\"$\"}\r\n            />\r\n\r\n            <button onClick={e => history.push('/payment')}>Proceed to Checkout</button>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Subtotal;\r\n","C:\\Users\\FS Web Dev\\Desktop\\Web_Dev_Main\\Other_Projects\\Amazon_React_Clone\\amazon-clone\\src\\reducer.js",[],"C:\\Users\\FS Web Dev\\Desktop\\Web_Dev_Main\\Other_Projects\\Amazon_React_Clone\\amazon-clone\\src\\StateProvider.jsx",[],"C:\\Users\\FS Web Dev\\Desktop\\Web_Dev_Main\\Other_Projects\\Amazon_React_Clone\\amazon-clone\\src\\CheckoutProduct.jsx",["91","92"],"import React, { useEffect } from 'react';\r\nimport './CheckoutProduct.css';\r\nimport { UseStateValue } from './StateProvider.jsx';\r\n\r\nfunction CheckoutProduct({id, image, title, price, rating, hide_button}) {\r\n\r\n    const [{basket, user}, dispatch] = UseStateValue();\r\n\r\n    useEffect(() => {\r\n        \r\n        if(!user)\r\n        {\r\n            dispatch({\r\n\r\n                type: 'EMPTY_BASKET',\r\n            })\r\n        }\r\n    }, [user]);\r\n\r\n    const remove_from_basket = () => {\r\n\r\n        // Remove the item from the basket\r\n        dispatch({\r\n\r\n            type: 'REMOVE_FROM_BASKET',\r\n            id: id,\r\n        })\r\n    };\r\n\r\n    return (\r\n        <div className=\"checkout-product\">\r\n            <img className=\"checkout-product-image\" src={image} alt=\"\"/>\r\n\r\n            <div className=\"checkout-product-info\">\r\n                <p className=\"checkout-product-title\">{title}</p>\r\n\r\n                <p className=\"checkout-product-price\">\r\n                    <small>$</small>\r\n                    <strong>{price.toFixed(2)}</strong>\r\n                </p>\r\n\r\n                <div className=\"checkout-product-rating\">\r\n                    {Array(rating).fill().map((_, i) => {\r\n                        \r\n                        return <p>⭐</p>\r\n                    })}\r\n                </div>\r\n\r\n                {!hide_button && (\r\n\r\n                    <button onClick={remove_from_basket}>Remove From Basket</button>\r\n                )}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default CheckoutProduct;\r\n","C:\\Users\\FS Web Dev\\Desktop\\Web_Dev_Main\\Other_Projects\\Amazon_React_Clone\\amazon-clone\\src\\Login.jsx",[],"C:\\Users\\FS Web Dev\\Desktop\\Web_Dev_Main\\Other_Projects\\Amazon_React_Clone\\amazon-clone\\src\\firebase.js",[],"C:\\Users\\FS Web Dev\\Desktop\\Web_Dev_Main\\Other_Projects\\Amazon_React_Clone\\amazon-clone\\src\\Payment.jsx",["93","94","95"],"import React, { useEffect, useState} from 'react';\r\nimport { Link, useHistory } from 'react-router-dom';\r\nimport CheckoutProduct from './CheckoutProduct.jsx';\r\nimport \"./Payment.css\";\r\nimport { UseStateValue } from './StateProvider.jsx';\r\nimport { CardElement, useElements, useStripe } from '@stripe/react-stripe-js';\r\nimport { get_basket_total } from './reducer.js';\r\nimport CurrencyFormat from 'react-currency-format';\r\nimport instance from './axiosConfig.js';\r\nimport { db } from 